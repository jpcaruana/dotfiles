[user]
	name = Jean-Philippe CARUANA
	email = jp@target2sell.com
[color]
	ui = auto
	interactive = auto
[core]
	editor = vim
	whitespace = -trailing-space
[diff]
	mnemonicPrefix = true
	wordRegex = .
[grep]
	extendedRegexp = true
[log]
	abbrevCommit = true
[status]
	submoduleSummary = true
	showUntrackedFiles = all
[push]
	default = simple
[alias]
	last = cat-file commit HEAD
	cm = commit -am\"[refactor] code mort\"
	duplication = commit -am\"[refactor] code mort : duplication\"
	format = commit -am\"[refactor] format\"
	rename = commit -am\"[refactor] rename\"
	st = status --short
	co = checkout
	br = branch -a --verbose
	rb = rebase
	rbi = rebase --interactive
	topo-log = log --oneline --graph --all --decorate
	topo-log-branch = log --oneline --graph --decorate
	k = !gitk
	unstage = reset HEAD
	uncommit = reset --soft HEAD^
	count = shortlog -s -n
[mergetool]
	keepBackup = false
	keepTemporaries = false
[mergetool "p4merge"]
	cmd = p4merge $BASE $LOCAL $REMOTE $MERGED
[merge]
	tool = p4merge
